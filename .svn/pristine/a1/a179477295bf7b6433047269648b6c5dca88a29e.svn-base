    <select id="getImportedProjectMachineInfo" resultType="baseMachineMng.vo.ImportProjectMachineInfoReturnVO">
	SELECT
		t0.XM_USER_ID AS project_ID ,
		t2.PROJECT_NAME AS project_Name ,
		t3.GS_NAME AS project_CompanyName ,
		t1.APPLY_TIME AS lastImportTime ,
		count(t1.PROJECT_ID) AS machine_importNum ,
		sum(
			CASE
			WHEN t1.APPLY_STATE = 1 THEN
				1
			ELSE
				0
			END
		) AS machine_passNum ,
		sum(
			CASE
			WHEN t1.APPLY_STATE = 2 THEN
				1
			ELSE
				0
			END
		) AS machine_notPassNum
	FROM
		XM_GYS t0
	LEFT JOIN PROJECT t2 ON t0.XM_USER_ID = t2.PROJECT_ID
	LEFT JOIN COMPANY t3 ON t2.COMPANY_ID = t3.GS_ID
	LEFT JOIN PROJECT_INSTRUMENT_INFO_TEMP t1 ON t0.XM_USER_ID = t1.PROJECT_ID
	WHERE
		t0.GYS_USER_ID = #{param1}
	AND t0.APPLY_STATE = 2
	GROUP BY
